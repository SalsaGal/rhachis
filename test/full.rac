.text {
    pub fn _start() {
        ax = 5;
        double_ax();
    }

    // Registers following : are not preserved, so if they are edited inside
    // the function, that edit is permanent, otherwise it is saved using the stack
    fn double_ax() : ax {
        ax += ax;
    }
}

